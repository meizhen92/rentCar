<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
	namespace="com.mysite.booreung.Reservation.mapper.FileInfoMapper">
	<insert id="insertFileInfo" useGeneratedKeys="true"
		keyProperty="file_no">
		insert into tb_fileinfo
		(
		file_name
		,file_extension
		,file_route
		,file_size
		) values (
		#{file_name}
		,#{file_extension}
		,#{file_route}
		,#{file_size}
		)
	</insert>
</mapper>